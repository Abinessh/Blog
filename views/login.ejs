<% include ./partials/header %> <% include ./partials/nav %>
<section>
  <div class="container">
    <div class="login-form">
      <h1>Sign in</h1>
      <form action="/login" method="POST">
        <!-- <i class="fa fa-user" aria-hidden="true"></i> -->
        <input
          type="text"
          placeholder="&#xf007; username"
          name="username"
          required
        />
        <!-- <i class="fa fa-lock" aria-hidden="true"></i> -->
